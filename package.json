{
  "name": "redux-setstate-reducer",
  "version": "0.0.0",
  "description": "Synchronus and functional setState with redux",
  "main": "lib/index.js",
  "scripts": {
    "build": "del lib && rollup -c",
    "jest": "jest",
    "lint": "eslint src",
    "pub": "NODE_ENV=production npm run build && npm publish",
    "start": "del lib && rollup -c -w",
    "test": "npm run lint && npm run jest -- --coverage"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "6.24.1",
    "del-cli": "1.1.0",
    "eslint": "4.12.1",
    "eslint-plugin-jest": "21.4.0",
    "jest": "21.2.1",
    "redux": "3.7.2",
    "rollup": "0.52.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "4.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js}",
      "!**/lib/**/*.{js}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text"
    ],
    "verbose": true
  },
  "author": "Romello Goodman <romellogood@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romellogoodman/redux-setstate-reducer.git"
  },
  "bugs": {
    "url": "https://github.com/romellogoodman/redux-setstate-reducer/issues"
  },
  "keywords": [
    "react",
    "redux",
    "redux-actions",
    "redux-reducers"
  ]
}
